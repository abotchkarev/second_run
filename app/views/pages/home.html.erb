<%#
# Viewer 'pages#home'
%>
<%  if signed_in? %>
  <div id="create_app">
    <%=  render 'appointments/create'%> 
  </div>
  <div id="show_active">
    <%=  render :partial => 'appointments/all_active' %>
  </div>
  <div id="show_history">
    <%=  render  'appointments/history' %>
  </div> 
<% else %>
  <h1>
    <%= @title %>
  </h1>
  <%=  link_to "Please sign in", signin_path %>
<% end %>

