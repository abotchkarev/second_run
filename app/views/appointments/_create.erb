<%#
# Partial "appinntments/create" 
# creates new active paused appointment.
# Called from 'pages#home' viewer
%>
<%= form_for @new_appointment  do |f| %>
  <%= f.label :relationship_id, "Select project to work with" %>
  <%= f.select :relationship_id, options_for_select(@projects_to_add) %>
  <%= f.submit "Start" %>
<% end %>

